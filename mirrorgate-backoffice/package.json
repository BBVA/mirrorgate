{
  "name": "mirrorgate-backoffice",
  "version": "0.0.1",
  "description": "Managment application for MirrorGate's dashboards",
  "scripts": {
    "build": "webpack",
    "build-aot": "npm run ngc && webpack --config webpack.aot.config.js && npm run cleanup",
    "cleanup": "./node_modules/.bin/rimraf src/app/**/*.ngfactory.ts src/app/**/*.ngstyle.ts src/**/*.ngsummary.json",
    "ngc": "node_modules/.bin/ngc -p tsconfig.aot.json",
    "watch": "npm run build -- --watch",
    "server": "webpack-dev-server --inline --progress --port 3000 --content-base src",
    "server-aot": "webpack-dev-server --inline --progress --port 3000 --content-base src --config webpack.aot.config.js",
    "start": "npm run server",
    "start-aot": "npm run server-aot",
    "test": "echo ToDo"
  },
  "contributors": [
    "Rob Wormald <robwormald@gmail.com>",
    "PatrickJS <github@gdi2290.com>"
  ],
  "license": "MIT",
